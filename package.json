{
  "name": "redux-first-router-demo",
  "description": "Universal Redux-First Router 2.0 Demo",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "James Gillmore <james@faceyspacey.com>",
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development babel-watch server/index.js",
    "start:prod": "npm run build && npm run serve",
    "serve": "cross-env NODE_ENV=production node buildServer/index.js",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "rimraf buildClient && cross-env NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server": "rimraf buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel server/index.js -o buildServer/index.js && babel server/api.js -o buildServer/api.js",
    "clean": "rimraf buildClient buildServer",
    "cm": "git-cz",
    "lint": "eslint --fix src server webpack",
    "format": "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && npm run lint"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.1.3",
    "express": "^4.17.1",
    "express-no-favicons": "^0.0.1",
    "fetch-everywhere": "^1.0.5",
    "history": "^4.9.0",
    "node-fetch": "^2.6.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^6.0.0",
    "react-universal-component": "^4.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-first-router": "^2.1.2",
    "redux-first-router-link": "2.1.1",
    "regenerator-runtime": "^0.13.2",
    "reselect": "^3.0.1",
    "transition-group": "^0.0.2",
    "webpack-flush-chunks": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-universal-import": "^4.0.0",
    "babel-watch": "^7.0.0",
    "commitizen": "^3.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "extract-css-chunks-webpack-plugin": "^4.5.2",
    "flow-bin": "^0.100.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.0",
    "prop-types": "^15.7.2",
    "react-hot-loader": "4.9.0",
    "rimraf": "^2.6.3",
    "stats-webpack-plugin": "^0.7.0",
    "travis-github-status": "^1.6.3",
    "webpack": "^4.33.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-hot-server-middleware": "^0.6.0",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --semi=false --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --verbose"
    }
  }
}
